    "xterm": "^5.2.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-addon-unicode11": "^0.5.0",
    "xterm-addon-web-links": "^0.8.0"
  },
  "devDependencies": {
    "@btwiuse/serve": "1.0.2",
    "@types/node": "18.11.0",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.4",
    "@types/styled-components": "^5.1.26",
    "concurrently": "8.2.0",
    "esbuild": "0.18.0",
    "rimraf": "5.0.1",
    "typescript": "5.1.3"
  }
